{
	"manifest_version": 2,
	"name": "Jamstash (Beta)",
	"description": "HTML5 Player for Subsonic & Archive.org",
	"version": "3.0.8",
	"app": {
		"launch": {
			"web_url": "http://jamstash.com/beta"
		}
	},
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"permissions": [
		"unlimitedStorage",
		"notifications",
		"http://*/*", 
		"https://*/*"
	]
}